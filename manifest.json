{
    "manifest_version": 2,
    "name": "Where Does This Lead?",
    "version": "1.0.0",
    "description": "WDTL? automatically unshortens links.",
    "permissions": [
        "*://*/*"
    ],
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["unshorten.js"]
        }
    ]
}